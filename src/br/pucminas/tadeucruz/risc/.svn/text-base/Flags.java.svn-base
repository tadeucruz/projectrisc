package br.pucminas.tadeucruz.risc;

public class Flags {

    private int Neg = 0;
    private int Zero = 0;
    private int Carry = 0;
    private int NegZero = 0;
    private int Overflow = 0;

    public int getCarry() {
        return Carry;
    }

    public void setCarry(int Carry) {
        this.Carry = Carry;
    }

    public int getNeg() {
        return Neg;
    }

    public void setNeg(int Neg) {
        this.Neg = Neg;
    }

    public int getNegZero() {
        return NegZero;
    }

    public void setNegZero(int NegZero) {
        this.NegZero = NegZero;
    }

    public int getOverflow() {
        return Overflow;
    }

    public void setOverflow(int Overflow) {
        this.Overflow = Overflow;
    }

    public int getZero() {
        return Zero;
    }

    public void setZero(int Zero) {
        this.Zero = Zero;
    }
}
